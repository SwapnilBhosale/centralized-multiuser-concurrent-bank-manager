#ifndef SERVER1_OBSERVER_H_
#define SERVER1_OBSERVER_H_


class Observer {


public:
	virtual void notify(char *data)=0;
	virtual ~Observer() {
		std::cout<<"destructor of observer called"<<std::endl;
	}
};


#endif /* SERVER1_OBSERVER_H_ */
