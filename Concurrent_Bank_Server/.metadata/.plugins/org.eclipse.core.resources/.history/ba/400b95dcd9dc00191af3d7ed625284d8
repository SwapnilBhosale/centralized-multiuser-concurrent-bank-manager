#include "ServerGUI.h"

Gui::Gui(){
	app =Gtk::Application::create(argc, argv, "org.gtkmm.examples.base");
	window.set_default_size(200, 200);
}

Gui::create_window(int argc, char **argv) {
	return app->run(window);
}
