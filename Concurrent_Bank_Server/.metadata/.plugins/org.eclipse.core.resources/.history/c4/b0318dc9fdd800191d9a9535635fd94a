#include "ServerSock.h"

ServerSock::ServerSock() {
	this->port = 8080;
	sockfd = NULL;

}

ServerSock::ServerSock(unsigned int port) {
	this->port = port;
	sockfd = NULL;
}

ServerSock::~ServerSock() {
}

ServerSock::init(unsigned int port)

